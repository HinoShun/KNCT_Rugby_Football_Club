<div class="main_">

  <div class="contents">

    <div class="session">

      <div class="form">

        <%= form_with model: @user, url: user_session_path, class: 'registration-main', local: true do |f| %>
          
          <h3 class='form-header-text'>
            管理者情報入力
          </h3>

          <div class="form_email">
            <%= f.email_field :email, placeholder:"  メールアドレス", autofocus: true %>
          </div>

          <div class="form_pass">
            <%= f.password_field :password, placeholder:"  パスワード" %>
          </div>
            
          <div class="form_submit">  
            <%= f.submit "ログイン" , class: :form_btn %>
          </div>
        
        <% end %>

      </div>

    </div>

  </div>

</div>
